@section styles {
    <environment names="Development">
        <link href="~/css/GrafikaApp.StickDraw.css" rel="stylesheet" type="text/css">
    </environment>
    <environment names="Production">
        <link rel="stylesheet" href="~/css/GrafikaApp.Bundle.StickDraw.min.css"
              asp-fallback-href="~/css/GrafikaApp.StickDraw.css"
              asp-fallback-test-class="btn" asp-fallback-test-property="border" asp-fallback-test-value="none" />
    </environment>
}


@section scripts {
    <environment names="Development">
        <script type="text/javascript" src="~/js/jquery.easing.js"></script>
        <script type="text/javascript" src="~/js/GrafikaApp.Stickdraw.js"></script>
    </environment>
    <environment names="Production">
        <script type="text/javascript" src="~/js/GrafikaApp.Bundle.StickDraw.min.js"></script>
    </environment>
}